{
    "testing": {
        "default cfg loaded": true,
        "overridden setting": false
    },

    "paths": {
        "content root": ".",
        "posts subdir": "posts",
        "pages subdir": "pages",

        "output root": "output",
        "templates root": "templates",
        "extensions root": "extensions",

        "post path template": "{content.date:%Y/%m}/{content.slug}/index.html",
        "page path template": "{content.slug}/index.html",

        "index pages path template": "page-{content.page_number}/index.html",
        "archives path template": "archives/index.html",
        "rss path template": "rss.xml",
        "json feed path template": "feed.json",
        "sitemap path template": "sitemap.xml"
    },

    "templates": {
        "post": "post.html",
        "page": "page.html",
        "index": "index.html",
        "archives": "archives.html",
        "rss": "majestic-rss.xml",
        "sitemap": "majestic-sitemap.xml"
    },

    "dates": {
        "format": "%Y-%m-%d %H:%M",
        "timezone": "UTC"
    },

    "index": {
        "paginate": true,
        "posts per page": 5
    },

    "feeds": {
        "number of posts": 10,
        "rss": {
            "generator": "majestic (https://github.com/robjwells/majestic)"
        },
        "json": {}
    },

    "site": {},

    "markdown": {
        "extensions": {}
    },

    "resources": [],

    "jinja": {
        "auto_reload": false
    },

    "preview": {
        "browser": "default"
    }
}
